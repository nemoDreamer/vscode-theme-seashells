  "tokenColors": [
    {
      "scope": "string",
      "settings": {
        "foreground": "#4b310c"
      }
    },
    {
      "scope": "string.template",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "string.template punctuation",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#fca02f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "constant.language.json, constant.language.import-export-all",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "support.variable, support.constant",
      "settings": {
        "foreground": "#4b310c"
      }
    },
    {
      "scope": "variable, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#4b310c"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.object.property, meta.field.declaration string, variable.other.object.property",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "support.variable.property, variable.other.property",
      "settings": {
        "foreground": "#68d4f1"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "modifier, support.type.object",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#fca02f"
      }
    },
    {
      "scope": "keyword.control.loop, keyword.control.conditional, keyword.control.trycatch",
      "settings": {
        "foreground": "#fca02f"
      }
    },
    {
      "scope": "keyword.control.import, keyword.control.from, keyword.control.export, keyword.control.default",
      "settings": {
        "foreground": "#251400",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#68d4f1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "new.expr entity.name.function",
      "settings": {
        "foreground": "#251400",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type, storage.modifier",
      "settings": {
        "foreground": "#027c9b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#4b310c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#68d4f1"
      }
    },
    {
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#251400"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#a3cedd",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#68d4f1"
      }
    },
    {
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#68d4f1"
      }
    },
    {
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#50a3b5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "source.css",
      "settings": {
        "foreground": "#fca02f"
      }
    },
    {
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "meta.tag entity.other.attribute-name",
      "settings": {
        "foreground": "#027c9b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#fca02f"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#4b310c"
      }
    },
    {
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#9fa8b2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#251400",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#251400"
      }
    },
    {
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#4b310c"
      }
    },
    {
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#027c9b"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "storage.modifier.ts, entity.name.type.module.ts, support.type.primitive.ts",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#50a3b5"
      }
    },
    {
      "scope": "punctuation.separator.parameter",
      "settings": {
        "foreground": "#a3cedd"
      }
    },
    {
      "scope": "meta.brace, punctuation.definition.parameters, punctuation.definition.block, punctuation.terminator.statement, punctuation.separator.comma, punctuation.section.embedded",
      "settings": {
        "foreground": "#a3cedd"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#8cb7bf"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#fca02f"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#c94a1c"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#027c9b"
      }
    }
  ]
}
